{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "dom",
      "es2015",
      "es2015.promise"
    ],
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "types": [
      "vue-typescript-import-dts"
    ]
  },
  "include": [
      "App/**/*.ts"
  ]
}
